<html>

    <head>
        <meta charset="utf-8">
        <title>Pagina Principala | Elena Cuza</title>
        <link rel="stylesheet" href="main.css">
        <!-- JavaScript Bundle with Popper -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"></script>
        <script src="main.js"></script>
    </head>

    <body> <div class="wrapper">
       
        <div class="nav">
        <ul>
    
            <li><a href="resurse">Resurse</a></li>
            <li><a  href="catalog">Catalog</a></li>
            <li><a href="mesagerie">Mesagerie</a></li>
            
            
        </ul>
        </div>
        <br>
        <div class="tabel">
            <table class="table table-bordered">
                <tr>
                    <th>Materie</th>
                    <th>Note</th>
                    <th>Medie</th>
                  </tr>
                  

                  <% for(var i=0;i<materii.length;i++) {%>
                      <tr>
                          <td><%=materii[i] %></td>
                          <td>- <button style="float:right;" type="button" class="btn btn-success" onclick="adaugareNota()">Adauga</button></td>
                          <td>- <button style="float:right;" type="button" class="btn btn-danger" onclick="adaugareNota()" >Incheie</button></td>
                      </tr>
                      <tr class="separator" />
                  <% } %>

            </table>
        </div>
      
       
    </body>
</html>